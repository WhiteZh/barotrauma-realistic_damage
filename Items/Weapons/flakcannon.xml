    <Item category="Weapon" hideinmenus="true" identifier="flakboltgravity" name="__flakboltgravity__" scale="1" sonarsize="2">
      <Sprite depth="0.55" sourcerect="143,414,113,9" texture="Content/Items/Weapons/weapons_new.png"/>
      <Body density="20" height="10" width="170"/>
      <Holdable aimable="false" handle1="-10,0" handle2="10,0" holdpos="0,-50" slots="RightHand+LeftHand"/>
      <Projectile characterusable="false" damagedoors="true" launchimpulse="0.0">
        <Attack itemdamage="15" levelwalldamage="100" penetration="0.1" severlimbsprobability="0.25" structuredamage="50">
          <Affliction identifier="internaldamage" strength="70"/>
          <Affliction identifier="stun" strength="8"/>
        </Attack>
        <StatusEffect lifetime="2" target="This" type="OnActive">
          <ParticleEmitter anglemax="5" anglemin="-5" copyentityangle="true" particle="ammotrailwater" particleamount="5" scalemax="1.5" scalemin="0.9" velocitymax="-100" velocitymin="-10"/>
        </StatusEffect>
        <StatusEffect target="This" type="OnBroken">
          <Conditional Removed="false"/>
          <SpawnItem aimspread="0" count="1" identifier="flakshrapnelgravity" rotation="0" rotationtype="Collider" spawnposition="This"/>
          <ParticleEmitter anglemax="360" anglemin="0" copyentityangle="true" particle="shrapnel" particleamount="20" scalemax="2" scalemin="1" velocitymax="2000" velocitymin="100"/>
          <RemoveItem/>
        </StatusEffect>
        <StatusEffect Condition="-100" delay="0.25" stackable="false" target="This" type="OnActive"/>
        <StatusEffect Condition="-100" stackable="false" target="This" type="OnImpact"/>
        <!-- child component of the Projectile = only active when the projectile is active -->
        <MotionSensor DetectOwnMotion="false" IgnoreDead="True" canbeselected="false" minimumvelocity="0" range="125" target="Monster" updateinterval="0.025">
          <StatusEffect checkconditionalalways="true" condition="-100" disabledeltatime="true" stackable="false" target="This" type="OnNotContained">
            <Conditional MotionDetected="eq true" targetitemcomponent="MotionSensor"/>
          </StatusEffect>
        </MotionSensor>
      </Projectile>
    </Item>
